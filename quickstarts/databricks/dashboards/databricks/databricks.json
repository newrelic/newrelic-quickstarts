            "title": "Cluster Load",
            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "SELECT average(loadAverageOneMinute), uniqueCount(hostname) AS 'Hosts', sum(processorCount) AS 'CPUs' FROM SystemSample  since 15 minutes ago until 1 minute ago TIMESERIES"
                }
              ]
            },
            "linkedEntityGuids": null
          },
