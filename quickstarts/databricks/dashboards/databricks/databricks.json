            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric select uniqueCount(spark.job.StageIds) AS 'Jobs Running' where spark.job.Status = 'RUNNING' since 1 minute ago "
                }
              ]
            },
            "linkedEntityGuids": null
