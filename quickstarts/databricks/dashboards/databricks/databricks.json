            "title": "Tasks Running",
            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric SELECT round(rate(sum(spark.job.numactivetasks), 5 seconds)) as 'Tasks Running' WHERE spark.job.Status = 'RUNNING'  since 1 minute ago"
                }
              ]
            },
            "linkedEntityGuids": null
