            },
            "title": "Workers",
            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric SELECT uniqueCount(spark.executor.ID) AS '# of Workers' where spark.executor.ID != 'driver' since 1 minute ago"
                }
              ]
            },
            "linkedEntityGuids": null
          },
