            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "from SystemSample SELECT latest(cpuPercent) as 'CPU %', latest(coreCount) as 'Cores' FACET hostname where hostname LIKE '%-%-%-%-%-%-%' LIMIT 50 SINCE 1 minute ago  "
                }
              ]
            },
            "linkedEntityGuids": null
