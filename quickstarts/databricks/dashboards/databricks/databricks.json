            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric select filter(uniqueCount(spark.job.StageIds), WHERE spark.job.Status = 'RUNNING') as 'Stages Running' since 1 minute ago "
