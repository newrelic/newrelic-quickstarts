            "title": "Spark Application Count",
            "rawConfiguration": {
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric SELECT uniqueCount(spark.app.ID) AS '# of Apps' since 1 hour ago"
                }
              ]
            },
