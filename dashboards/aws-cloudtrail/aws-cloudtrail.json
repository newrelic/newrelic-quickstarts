                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT COUNT(*) FROM InfrastructureEvent WHERE provider='CloudTrail' FACET awsRegion SINCE 1 WEEK AGO"
                            }
                        ]
                    }
                }
