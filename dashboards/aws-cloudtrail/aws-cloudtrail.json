                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT COUNT(*) FROM InfrastructureEvent WHERE provider='CloudTrail' (errorCode IS NOT NULL OR errorMessage IS NOT NULL) FACET eventName SINCE 1 WEEK AGO TIMESERIES AUTO"
