              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT average(transmitBytesPerSecond) AS 'Bytes Transmitted' FROM NetworkSample TIMESERIES FACET entityAndInterface LIMIT 100 SINCE 2 days ago"
              }
