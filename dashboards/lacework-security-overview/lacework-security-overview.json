            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT average(receiveBytesPerSecond) AS 'Bytes Received' FROM NetworkSample TIMESERIES FACET entityAndInterface LIMIT 100 SINCE 2 days ago"
              }
            ],
            "yAxisLeft": {
