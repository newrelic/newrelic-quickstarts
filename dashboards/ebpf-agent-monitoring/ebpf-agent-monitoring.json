{
    "name": "eBPF agent monitoring",
    "description": null,
    "pages": [
        {
            "name": "Agents",
            "description": null,
            "widgets": [
                {
                    "title": "Agent entities count",
                    "layout": {
                        "column": 1,
                        "row": 1,
                        "width": 4,
                        "height": 9
                    },
                    "linkedEntityGuids": null,
                    "visualization": {
                        "id": "viz.table"
                    },
                    "rawConfiguration": {
                        "facet": {
                            "showOtherSeries": false
                        },
                        "nrqlQueries": [
                            {
                                "accountIds": [],
                                "query": "FROM Log SELECT latest(monitoredEntitiesCount), latest(discoveredEntitiesCount) WHERE recordType = 'eBPF-Agent-Metadata'  AND agentName = 'NewRelic eBPF Agent' FACET agentId "
                            }
                        ],
                        "platformOptions": {
                            "ignoreTimeRange": false
                        }
                    }
                },
                {
                    "title": "Agent CPU percent",
                    "layout": {
                        "column": 5,
                        "row": 1,
                        "width": 4,
                        "height": 3
                    },
                    "linkedEntityGuids": null,
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "chartStyles": {
                            "lineInterpolation": "linear"
                        },
                        "facet": {
                            "showOtherSeries": false
                        },
                        "legend": {
                            "enabled": true
                        },
                        "markers": {
                            "displayedTypes": {
                                "criticalViolations": false,
                                "deployments": true,
                                "relatedDeployments": true,
                                "warningViolations": false
                            }
                        },
                        "nrqlQueries": [
                            {
                                "accountIds": [],
                                "query": "FROM Log SELECT latest(agentProcessCPUPercent) WHERE recordType = 'eBPF-Agent-Metadata' AND agentName = 'NewRelic eBPF Agent' FACET agentId TIMESERIES "
                            }
                        ],
                        "platformOptions": {
                            "ignoreTimeRange": false
                        },
                        "thresholds": {
                            "isLabelVisible": true
                        },
                        "yAxisLeft": {
                            "zero": true
                        },
                        "yAxisRight": {
                            "zero": true
                        }
                    }
                },
                {
                    "title": "Agent memory (MB)",
                    "layout": {
                        "column": 9,
                        "row": 1,
                        "width": 4,
                        "height": 3
                    },
                    "linkedEntityGuids": null,
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "chartStyles": {
                            "lineInterpolation": "linear"
                        },
                        "facet": {
                            "showOtherSeries": false
                        },
                        "legend": {
                            "enabled": true
                        },
                        "markers": {
                            "displayedTypes": {
                                "criticalViolations": false,
                                "deployments": true,
                                "relatedDeployments": true,
                                "warningViolations": false
                            }
                        },
                        "nrqlQueries": [
                            {
                                "accountIds": [],
                                "query": "FROM Log SELECT latest(agentProcessMemoryMB) WHERE recordType = 'eBPF-Agent-Metadata' AND agentName = 'NewRelic eBPF Agent' FACET agentId TIMESERIES "
                            }
                        ],
                        "platformOptions": {
                            "ignoreTimeRange": false
                        },
                        "thresholds": {
                            "isLabelVisible": true
                        },
                        "yAxisLeft": {
                            "zero": true
                        },
                        "yAxisRight": {
                            "zero": true
                        }
                    }
                },
                {
                    "title": "Agent deployment type",
                    "layout": {
                        "column": 5,
                        "row": 4,
                        "width": 4,
                        "height": 3
                    },
                    "linkedEntityGuids": null,
                    "visualization": {
                        "id": "viz.pie"
                    },
                    "rawConfiguration": {
                        "facet": {
                            "showOtherSeries": true
                        },
                        "legend": {
                            "enabled": true
                        },
                        "nrqlQueries": [
                            {
                                "accountIds": [],
                                "query": "FROM Log SELECT uniqueCount(agentId) WHERE recordType = 'eBPF-Agent-Metadata' AND agentName = 'NewRelic eBPF Agent' FACET deploymentType "
                            }
                        ],
                        "platformOptions": {
                            "ignoreTimeRange": false
                        }
                    }
                },
                {
                    "title": "Agent version",
                    "layout": {
                        "column": 9,
                        "row": 4,
                        "width": 4,
                        "height": 3
                    },
                    "linkedEntityGuids": null,
                    "visualization": {
                        "id": "viz.table"
                    },
                    "rawConfiguration": {
                        "facet": {
                            "showOtherSeries": false
                        },
                        "nrqlQueries": [
                            {
                                "accountIds": [],
                                "query": "FROM Log SELECT latest(version) AS 'Agent version' WHERE recordType = 'eBPF-Agent-Metadata' AND agentName = 'NewRelic eBPF Agent' FACET agentId "
                            }
                        ],
                        "platformOptions": {
                            "ignoreTimeRange": false
                        }
                    }
                },
                {
                    "title": "Agent deployed on",
                    "layout": {
                        "column": 5,
                        "row": 7,
                        "width": 8,
                        "height": 3
                    },
                    "linkedEntityGuids": null,
                    "visualization": {
                        "id": "viz.table"
                    },
                    "rawConfiguration": {
                        "facet": {
                            "showOtherSeries": false
                        },
                        "nrqlQueries": [
                            {
                                "accountIds": [],
                                "query": "FROM Log SELECT latest(OSName), latest(architecture), latest(kernelVersion) WHERE recordType = 'eBPF-Agent-Metadata' AND agentName = 'NewRelic eBPF Agent' FACET agentId"
                            }
                        ],
                        "platformOptions": {
                            "ignoreTimeRange": false
                        }
                    }
                }
            ]
        },
        {
            "name": "Monitored entities",
            "description": null,
            "widgets": [
                {
                    "title": "Reporting entities",
                    "layout": {
                        "column": 1,
                        "row": 1,
                        "width": 12,
                        "height": 3
                    },
                    "linkedEntityGuids": null,
                    "visualization": {
                        "id": "viz.table"
                    },
                    "rawConfiguration": {
                        "facet": {
                            "showOtherSeries": false
                        },
                        "nrqlQueries": [
                            {
                                "accountIds": [],
                                "query": "FROM Log SELECT latest(processCPUPercent), latest(processMemoryMB), latest(pid), latest(language), latest(environment), latest(isAPMAttached) WHERE recordType = 'eBPF-Entity-Metadata' AND reporting = true FACET agentId, entityName "
                            }
                        ],
                        "platformOptions": {
                            "ignoreTimeRange": false
                        }
                    }
                }
            ]
        }
    ],
    "variables": []
}