                    },
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT average(`aws.elasticache.SwapUsage.byRedisCluster`) FROM Metric facet entity.name TIMESERIES 5 minutes UNTIL 5 minutes ago"
