            "widgets": [
                {
                    "title": "Top 5 buckets by number of objects",
                    "layout": {
                        "height": 3,
                        "width": 4,
                        "row": 1,
                        "column": 1
                    },
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT average(`aws.s3.NumberOfObjects`) FROM Metric TIMESERIES 1 day FACET entity.name SINCE 1 week ago until 1 day ago limit 5"
                            }
                        ]
                    }
                },
