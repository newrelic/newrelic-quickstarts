                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT average(`aws.sns.NumberOfMessagesPublished`) FROM Metric FACET `aws.sns.TopicName` timeseries 30 minutes since 3 days ago"
                            }
                        ]
                    }
                }
