            "nrqlQueries": [
              {
                "accountIds": [],
                "query": "FROM OpenAICompletion SELECT filter(sum(usage.total_tokens) * 0.0200 / 1000, where model LIKE '%davinci%') + filter(sum(usage.total_tokens) * 0.0020 / 1000, where model LIKE '%curie%') + filter(sum(usage.total_tokens) * 0.0005 / 1000, where model LIKE '%babbage%') + filter(sum(usage.total_tokens) * 0.0004 / 1000, where model LIKE '%ada%') as 'Cost (USD)'"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
