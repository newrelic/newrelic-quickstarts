        {
          "title": "Process Uptime (days)",
          "layout": {
            "column": 3,
            "row": 2,
            "width": 1,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT latest(influxdb_uptime_seconds)/60/60/24 as '' WHERE instrumentation.name = 'remote-write' AND (app_kubernetes_io_name LIKE '%influxdb2%' or app_newrelic_com_name LIKE '%influxdb2%') "
