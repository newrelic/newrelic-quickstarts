            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT apdex(newrelic.timeslice.value) AS `Apdex/Expressjs/POST//user/login` FROM Metric WHERE metricTimesliceName LIKE '%Apdex/Expressjs/POST//user/login%' AND appName LIKE '%mean%' TIMESERIES AUTO"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
