              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountIds": [686923],
                "query": "FROM Metric SELECT AVERAGE(`azure.network.azurefirewalls.Throughput`) AS 'Average Throughput' TIMESERIES AUTO"
              }
