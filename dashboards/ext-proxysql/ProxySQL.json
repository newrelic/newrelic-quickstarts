                "showOtherSeries": false
              },
              "legend": {
                "enabled": true
              },
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "SELECT latest(proxysql_client_connections_non_idle) as 'In-Use' FROM Metric WHERE instrumentation.name = 'remote-write' TIMESERIES AUTO"
                },
                {
                  "accountId": 0,
                  "query": "SELECT (latest(proxysql_client_connections_connected) - latest(proxysql_client_connections_non_idle)) as 'Idle' FROM Metric WHERE instrumentation.name = 'remote-write' TIMESERIES AUTO"
                }
              ],
              "platformOptions": {
