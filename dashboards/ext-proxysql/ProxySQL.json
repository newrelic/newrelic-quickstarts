                "showOtherSeries": false
              },
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric SELECT percentage(sum(proxysql_query_cache_count_get_total), WHERE status = 'ok') or 0 as '' WHERE instrumentation.name = 'remote-write' and (app_kubernetes_io_name LIKE '%proxysql%' or app_newrelic_com_name LIKE '%proxysql%') Facet instance"
